<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

<constant name="struts.objectFactory" value="spring"></constant>
	<package name="com.team512.start" extends="struts-default">
		<action name="loginAction" class="loginAction">
		<result name="success">/success.jsp</result>
		<result name="error">/index.html</result>
		</action>
		
		<action name="registerAction" class="registerAction">
		<result name="success">/index.html</result>
		<result name="error">/signup.html</result>
		</action></package>
		
		<package name="com.team512.check" namespace="/ajax/check" extends="struts-default">
		<action name="checkEmail" class="com.team512.action.EmailCheckAction"></action>
		</package>
</struts>    
